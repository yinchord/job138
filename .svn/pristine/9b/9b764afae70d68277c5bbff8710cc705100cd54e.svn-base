<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/navigationbar_bg" >

        <ImageButton
            android:id="@+id/button_left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:background="@null"
            android:src="@drawable/button_back_selector" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:src="@drawable/myresume" />

        <ProgressBar
            android:id="@+id/loading"
            style="?android:attr/progressBarStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/percent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView1"
            android:padding="5dp"
            android:text="(75%)"
            android:textColor="@color/white"
            android:textSize="16sp" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/header"
        android:background="@color/bg"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/menu_layout"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingBottom="48dp"
            android:paddingTop="16dp" >

            <TextView
                android:id="@+id/base_info"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="基本资料"
                android:textColor="@color/font_blue"
                android:textSize="14sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@drawable/line1" />

            <TextView
                android:id="@+id/contact_info"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="联系方式"
                android:textColor="@color/font_blue"
                android:textSize="14sp" />

            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@drawable/line1" />

            <TextView
                android:id="@+id/job_intent"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="求职意向"
                android:textColor="@color/font_blue"
                android:textSize="14sp" />

            <View
                android:id="@+id/View01"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@drawable/line1" />

            <TextView
                android:id="@+id/my_content_info"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="自我评价"
                android:textColor="@color/font_blue"
                android:textSize="14sp" />

            <View
                android:id="@+id/View02"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@drawable/line1" />

            <TextView
                android:id="@+id/edu_info"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="教育经历"
                android:textColor="@color/font_blue"
                android:textSize="14sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@drawable/line1" />

            <TextView
                android:id="@+id/train_info"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="培训经历"
                android:textColor="@color/font_blue"
                android:textSize="14sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@drawable/line1" />

            <TextView
                android:id="@+id/job_info"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="工作经历"
                android:textColor="@color/font_blue"
                android:textSize="14sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@drawable/line1" />

            <TextView
                android:id="@+id/ability_info"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="技能专长"
                android:textColor="@color/font_blue"
                android:textSize="14sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@drawable/line1" />

            <TextView
                android:id="@+id/language_info"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="语言能力"
                android:textColor="@color/font_blue"
                android:textSize="14sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@drawable/line1" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingBottom="48dp"
                android:paddingTop="16dp" >

                <TextView
                    android:id="@+id/base_info_show"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawablePadding="16dp"
                    android:drawableRight="@drawable/arrow_right"
                    android:gravity="right|center_vertical"
                    android:paddingRight="16dp"
                    android:textColor="@color/font_gray"
                    android:textSize="12sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@drawable/line1" />

                <TextView
                    android:id="@+id/contact_info_show"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawablePadding="16dp"
                    android:drawableRight="@drawable/arrow_right"
                    android:gravity="right|center_vertical"
                    android:paddingRight="16dp"
                    android:textColor="@color/font_gray"
                    android:textSize="12sp" />

                <View
                    android:id="@+id/View05"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@drawable/line1" />

                <TextView
                    android:id="@+id/job_intent_show"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawablePadding="16dp"
                    android:drawableRight="@drawable/arrow_right"
                    android:gravity="right|center_vertical"
                    android:paddingRight="16dp"
                    android:textColor="@color/font_gray"
                    android:textSize="12sp" />

                <View
                    android:id="@+id/View04"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@drawable/line1" />

                <TextView
                    android:id="@+id/my_contant_show"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawablePadding="16dp"
                    android:drawableRight="@drawable/arrow_right"
                    android:gravity="right|center_vertical"
                    android:paddingRight="16dp"
                    android:text="未填写"
                    android:textColor="@color/font_gray"
                    android:textSize="12sp" />

                <View
                    android:id="@+id/View03"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@drawable/line1" />

                <TextView
                    android:id="@+id/edu_info_show"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawablePadding="16dp"
                    android:drawableRight="@drawable/arrow_right"
                    android:gravity="right|center_vertical"
                    android:paddingRight="16dp"
                    android:text="未填写"
                    android:textColor="@color/font_gray"
                    android:textSize="12sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@drawable/line1" />

                <TextView
                    android:id="@+id/train_info_show"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawablePadding="16dp"
                    android:drawableRight="@drawable/arrow_right"
                    android:gravity="right|center_vertical"
                    android:paddingRight="16dp"
                    android:text="未填写"
                    android:textColor="@color/font_gray"
                    android:textSize="12sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@drawable/line1" />

                <TextView
                    android:id="@+id/job_info_show"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawablePadding="16dp"
                    android:drawableRight="@drawable/arrow_right"
                    android:gravity="right|center_vertical"
                    android:paddingRight="16dp"
                    android:text="未填写"
                    android:textColor="@color/font_gray"
                    android:textSize="12sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@drawable/line1" />

                <TextView
                    android:id="@+id/ability_info_show"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawablePadding="16dp"
                    android:drawableRight="@drawable/arrow_right"
                    android:gravity="right|center_vertical"
                    android:paddingRight="16dp"
                    android:text="未填写"
                    android:textColor="@color/font_gray"
                    android:textSize="12sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@drawable/line1" />

                <TextView
                    android:id="@+id/language_info_show"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:drawablePadding="16dp"
                    android:drawableRight="@drawable/arrow_right"
                    android:gravity="right|center_vertical"
                    android:paddingRight="16dp"
                    android:text="未填写"
                    android:textColor="@color/font_gray"
                    android:textSize="12sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@drawable/line1" />
            </LinearLayout>

            <FrameLayout
                android:id="@+id/content"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            </FrameLayout>
        </RelativeLayout>
    </LinearLayout>

</RelativeLayout>